!! Appearance
URxvt.depth: 32
URxvt.scrollBar: false
URxvt.font: xft:Hack:size=9
URxvt.boldFont: xft:Hack:size=9
URxvt.letterSpace: 0

!! Extensions
URxvt.perl-ext-common: default,matcher
!! matcher
URxvt.colorUL: #707880
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

