!! Appearance
urxvt.foreground: grey
urxvt.background: [80]#000000
urxvt.depth: 32
urxvt.scrollBar: false
urxvt.font: xft:Noto Sans:size=10
urxvt.boldFont: xft:Noto Sans:size=10
urxvt.letterSpace: -3
